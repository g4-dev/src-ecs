{
    "name": "My Server",
    "host": "localhost",
    "protocol": "sftp",
    "port": 2222,
    "username": "vagrant",
    "group": "vagrant",
    "password": "vagrant",
    "remotePath": "/data/ecs/www",
    "uploadOnSave": false,
    "syncMode": "full",
    "downloadOnOpen": true,
    "watcher": {
        "files": "**/*",
        "autoUpload": true,
        "autoDelete": true
    },
    "ignore": [
        "**/.vscode/**",
        "**/.idea/**",
        "**/.git/**",
        "**/.DS_Store",
        "**/.env",
        "**/.env.test",
        "**/node_modules/",
        "**/vendor/",
        "**/**.log"
    ]
}
